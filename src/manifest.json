{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["bundle.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Chess Game"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}

